{
  "name": "c8y-data-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint src/**",
    "fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "yarn lint && yarn build && jest --detectOpenHandles --forceExit --maxWorkers 2",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage  --detectOpenHandles --forceExit --maxWorkers 2",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@c8y/client": "^1016.0.307",
    "@golevelup/nestjs-rabbitmq": "^4.0.0",
    "@hokify/agenda": "^6.3.0",
    "@nestjs/common": "^10.1.0",
    "@nestjs/core": "^10.1.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.1.0",
    "@types/amqplib": "^0.10.1",
    "cron-parser": "^4.8.1",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "form-data": "^4.0.0",
    "human-interval": "^2.0.1",
    "luxon": "^3.3.0",
    "mongoose": "^7.4.0",
    "nest-typed-config": "^2.7.0",
    "nestjs-agenda-plus": "^2.0.0",
    "nestjs-minio-client": "^2.0.0",
    "node-fetch": "^2.6.12",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.1.10",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/luxon": "^3.3.1",
    "@types/minio": "^7.0.15",
    "@types/node": "^18.17.0",
    "@types/node-fetch": "^2.6.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "mongodb": "4.16.0",
    "mongodb-memory-server": "^8.13.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
